{
  "name": "practi-encuestas-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-stats": "npx webpack-bundle-analyzer dist/shell/stats.json --port 4000",
    "start-dist": "http-server dist/shell/browser --port 4100",
    "build": "ng build",
    "build-stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write \"**/*.ts\" \"**/*.html\" \"**/*.scss\" \"**/*.json\" \"**/*.js\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-architects/native-federation": "^21.0.2",
    "@angular/animations": "^20.3.0",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ionic/angular": "^8.7.15",
    "@kato-lee/utilities": "^1.0.0",
    "@ngrx/store": "^20.1.0",
    "@softarc/native-federation-node": "^3.3.4",
    "@toshida/cdk": "^20.2.14",
    "@toshida/material": "^20.2.14",
    "dayjs": "^1.11.19",
    "es-module-shims": "^1.5.12",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.13",
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2",
    "webpack-bundle-analyzer": "^5.1.0"
  }
}
